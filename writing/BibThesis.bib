@Article{Ulurlu2015,
  author                  = {Ulurlu, O. and Yildirim, U. and Basar, E.},
  title                   = {Analysis of grounding accidents caused by human error},
  journal                 = {Journal of Marine Science and Technology},
  year                    = {2015},
  volume                  = {23},
  number                  = {5},
  pages                   = {748-760},
  issn                    = {10232796},
  abbrev_source_title     = {J. Mar. Sci. Technol.},
  abstract                = {Despite modern bridge equipment, new technologies, and improved safety measures, maritime accidents still occur, and an analysis of their causes is essential in preventing future accidents. Ship groundings are one of the more frequent types of accidents encountered, and this study examines the maritime accident reports issued for grounded ships between 1993 and 2011. These were sourced using the International Maritime Organization's Global Integrated Shipping Information System and the reports published by the countries and relevant institutions that investigated the accidents. The grounding accidents are analysed using the Analytic Hierarchy Process, and the objective of this study is to act as an advisory paper on the prevention of grounding accidents involving human error. The results suggest that the most significant causes of these types of accidents are, lack of communication and coordination in Bridge Resource Management, position-fixing application errors, lookout errors, interpretation errors, use of improper charts, inefficient use of bridge navigation equipment, and fatigue. It is suggested that providing more education and training opportunities to seafarers, promoting widespread use on board of Electronic Chart Display and Information Systems, and improving seafarers' working hours and rest breaks would help to reduce grounding accidents significantly.},
  affiliation             = {Department of Maritime Transportation and Management Engineering, Karadeniz Technical University, Sürmene, Trabzon, Turkey},
  author_keywords         = {Accident analysis; Grounding accident; Human factor; Marine accident},
  correspondence_address1 = {Uʇurlu, O.; Department of Maritime Transportation and Management Engineering, Karadeniz Technical UniversityTurkey; email: ozkanugurlu24@hotmail.com},
  document_type           = {Article},
  doi                     = {10.6119/JMST-015-0615-1},
  language                = {English},
  publisher               = {National Taiwan Ocean University},
  source                  = {Scopus},
}

@Article{Akhatar2014,
  author                  = {Akhatar, M.J.},
  title                   = {Fatigue at sea - A manning problem},
  journal                 = {Journal of Maritime Research},
  year                    = {2014},
  volume                  = {11},
  number                  = {3},
  pages                   = {27-42},
  issn                    = {16974840},
  abbrev_source_title     = {J. Marit. Res.},
  abstract                = {Data about human and organizational factors from in-depth analysis of 93 grounding accident investigation reports are used to construct logistic regression models of human fatigue. A hierarchical model of the strongest fatigue-influencing factors in maritime transport is developed. Fatigue is most strongly influenced by less than adequate (LTA) manning levels and variable working conditions. LTA manning and Variable working conditions are shown to have strong influence on human fatigue on board. The regression models also show that offcers working 12-12 shifts have a significantly higher probability of having variable work hours which in turn have a negative impact on their fatigue levels. © SEECMAR. All rights reserved.},
  affiliation             = {Norwegian University of Science and Technology (NTNU), Norwegian institute of transport economics (TØI), Norway},
  author_keywords         = {Accident; Competitiveness; Human and organizational factors; Human fatigue; Logistic regression; Shift scheme},
