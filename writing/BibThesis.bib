  document_type           = {Article},
  doi                     = {10.1016/j.chemolab.2017.10.015},
  keywords                = {algorithm; Article; class imbalanced data; classification; controlled study; data analysis; leaving half of majority observations out and leaving one minority observation out; logistic regression analysis; mathematical analysis; measurement precision; Nasopharyngeal carcinoma dataset; precision recall curve; priority journal; simulation; sparse regularized logistic regression combined with subsampling; subsampling; Traumatic brain injury dataset; validation process},
