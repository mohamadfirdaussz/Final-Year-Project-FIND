@Article{Louie_200782,
  author                  = {Louie, V.W. and Doolen, T.L.},
  title                   = {A study of factors that contribute to maritime fatigue},
  journal                 = {Marine Technology},
  year                    = {2007},
  volume                  = {44},
  number                  = {2},
  pages                   = {82-92},
  issn                    = {00253316},
  __markedentry           = {[balandongiv:1]},
  abbrev_source_title     = {Mar Technol},
  abstract                = {Many advances have been made to shipping and the maritime industry over the last century. Despite these advances, errors that lead to injuries, accidents, and catastrophes continue to occur. Fatigue has been identified as a major contributor to these incidents. The purpose of this study was to investigate the causes of fatigue for mariners and, specifically, for deck watch officers (DWOs). A review of the literature was completed to identify factors that might impact the prevalence of fatigue in the maritime industry. Questionnaires and face-to-face interviews were used to collect data to evaluate the relationship among sleep, the mariner's work environment, and fatigue. It was found that advances in technology have reduced the number of personnel on the bridge and, subsequently, increased the workload for DWOs. With the multitude of DWO duties and responsibilities, current staffing levels are not always adequate for ensuring proper rest. While regulations that address hours of rest for mariners appear to be adequate, current regulations do not adequately address the challenges of dynamic schedules and human physiology. © Marine Technology.},
  affiliation             = {Human Element and Ship Design Division, U.S. Coast Guard Headquarters, Washington, DC, United States; Department of Industrial and Manufacturing Engineering, Oregon State University, United States},
  coden                   = {MARTA},
